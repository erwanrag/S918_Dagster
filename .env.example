# ============================================================================
# Dagster ETL - Environment Variables (EXAMPLE)
# ============================================================================
# Copier ce fichier vers .env et remplir avec vos vraies valeurs

# PostgreSQL
ETL_PG_HOST=localhost
ETL_PG_PORT=5432
ETL_PG_DATABASE=etl_db
ETL_PG_USER=postgres
ETL_PG_PASSWORD=your_password_here

# Dagster
DAGSTER_HOME=/data/dagster/dagster_home
DAGSTER_POSTGRES_URL=postgresql://postgres:your_password_here@localhost:5432/etl_db

# SFTP
ETL_SFTP_ROOT=/data/sftp_cbmdata01

# dbt
ETL_DBT_PROJECT=/data/dagster/workspace/dbt/etl_db

# Alerting (optionnel)
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your_webhook_url
SMTP_HOST=mail.proginov.fr
SMTP_PORT=25
ALERT_EMAIL_FROM=etl@cbm-analytics.fr
ALERT_EMAIL_TO=e.ragueneau@cbmcompany.com

# Logging
LOG_LEVEL=INFO